{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "A platform for users to ask questions and give answers to asked questions.",
  "main": "server/app.js",
  "scripts": {
    "postinstall": "npm run mig-two",
    "start": "babel-node server/app.js",
    "start-dev": "nodemon --exec babel-node server/app.js",
    "mig": "npm run create:table",
    "mig-two": "npm run create:table-two",
    "lint": "eslint --init --fix",
    "seed": "babel-node ./server/models/migrate.js",
    "create:table-two": "psql -U postgres -f  ./server/models/stackoverflowlite.sql",
    "create:table": "psql -f ./server/models/stackoverflowlite.sql postgres://bkiuragr:5L3w34ESbmek2qESWLImnkYy8cfGYbaE@baasu.db.elephantsql.com:5432/bkiuragr",
    "test": "npm run mig && nyc --reporter=html --reporter=text mocha --require babel-core/register --timeout 6000 server/test/**.js --exit",
    "test-local": "set NODE_ENV=local&& npm run mig-two && nyc --reporter=html --reporter=text mocha --require babel-core/register --timeout 6000 server/test/**.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucheg6/StackOverflow-lite.git"
  },
  "author": "Uche Akogwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ucheg6/StackOverflow-lite/issues"
  },
  "homepage": "https://github.com/ucheg6/StackOverflow-lite#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "validator": "^9.4.1",
    "pg": "^7.0.3",
    "should": "^13.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "request": "^2.88.0"
  }
}
